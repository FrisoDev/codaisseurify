<p><h1><%=@artist.name%></h1></p>
<p>Formed: <%=@artist.act_since.floor%></p>
<h4>Songs</h4>
<% @artist.songs.each do |song| %>
  <span>
    <ul>
      <li><%= song.name %></span></li>
      <%= link_to "Remove this song", songs_path, method: :destroy, data: { confirm: "Are you sure?" } %>
    </ul>
  </span>

<% end %>
<%= link_to "Remove this artist", artist_path(@artist[:id]), method: :delete, data: { confirm: "Are you sure?" } %>
