<%= form_for @artist, html: { multipart: true } do |f|  %>

 <div class="form-group">
     <%= f.label :name %>
     <%= f.text_field :name %>
 </div>
 <div class="form-group">
     <%= f.label :country %>
     <%= f.text_field :country %>
 </div>

 <button><%= f.submit "Submit" %></button>

<%end%>
