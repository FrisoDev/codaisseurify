<div class="gelul" style="text-align: center;">
<%= form_for @song, url: {action: "create"} do |f|  %>

 <div class="form-group">
     <%= f.label :name %>
     <%= f.text_field :name %>
 </div>
 <div class="form-group">
     <%= f.label :length_min %>
     <%= f.number_field :length_min %>

     <%= f.label :length_sec %>
     <%= f.number_field :length_sec %>
 </div>
 <div>
 <%= f.label :artist_id %>
   <%= f.collection_select(:artist_id, Artist.all, :id, :name) %>
</div>
   <div class="actions">
  <%= f.submit "Save", class: "btn btn-success" %>
</div>
<%end%>
</div>
