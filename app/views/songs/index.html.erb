<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1 class="page-header">Songs</h1>

    <% if notice.present? %>
      <div class="alert alert-info" role="alert">
        <%= notice %>
      </div>
    <% end %>

    <%= render "form", song: @song %>

    <hr />

    <table class="table table-striped" id="todoList">
      <%= render @songs %>
    </table>
  </div>
</div>
