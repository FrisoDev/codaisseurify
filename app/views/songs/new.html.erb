<div class="panel panel-default">
  <div class="panel-heading">
    Add a new song
  </div>

  <div class="panel-body">
 <%= form_for [:artist, @song] do |f| %>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>
    <div class="form-group">
        <%= f.label :Min %>
        <%= f.number_field :length_min %>

        <%= f.label :Sec %>
        <%= f.number_field :length_sec %>
    </div>
    <div class="actions">
   <%= f.submit "Save", class: "btn btn-success" %>
 </div>
    <% end %>
  </div>
</div>
